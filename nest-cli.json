{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/main/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/main/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/main",
  "projects": {
    "main": {
      "type": "application",
      "root": "apps/main",
      "entryFile": "main",
      "sourceRoot": "apps/main/src",
      "compilerOptions": {
        "tsConfigPath": "apps/main/tsconfig.app.json"
      }
    },
    "admin": {
      "type": "application",
      "root": "apps/admin",
      "entryFile": "main",
      "sourceRoot": "apps/admin/src",
      "compilerOptions": {
        "tsConfigPath": "apps/admin/tsconfig.app.json"
      }
    },
    "permissions": {
      "type": "library",
      "root": "libs/permissions",
      "entryFile": "index",
      "sourceRoot": "libs/permissions/src",
      "compilerOptions": {
        "tsConfigPath": "libs/permissions/tsconfig.lib.json"
      }
    },
    "users": {
      "type": "library",
      "root": "libs/users",
      "entryFile": "index",
      "sourceRoot": "libs/users/src",
      "compilerOptions": {
        "tsConfigPath": "libs/users/tsconfig.lib.json"
      }
    },
    "password": {
      "type": "library",
      "root": "libs/password",
      "entryFile": "index",
      "sourceRoot": "libs/password/src",
      "compilerOptions": {
        "tsConfigPath": "libs/password/tsconfig.lib.json"
      }
    },
    "token": {
      "type": "library",
      "root": "libs/token",
      "entryFile": "index",
      "sourceRoot": "libs/token/src",
      "compilerOptions": {
        "tsConfigPath": "libs/token/tsconfig.lib.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "prisma": {
      "type": "library",
      "root": "libs/prisma",
      "entryFile": "index",
      "sourceRoot": "libs/prisma/src",
      "compilerOptions": {
        "tsConfigPath": "libs/prisma/tsconfig.lib.json"
      }
    },
    "role": {
      "type": "library",
      "root": "libs/role",
      "entryFile": "index",
      "sourceRoot": "libs/role/src",
      "compilerOptions": {
        "tsConfigPath": "libs/role/tsconfig.lib.json"
      }
    },
    "sessions": {
      "type": "library",
      "root": "libs/sessions",
      "entryFile": "index",
      "sourceRoot": "libs/sessions/src",
      "compilerOptions": {
        "tsConfigPath": "libs/sessions/tsconfig.lib.json"
      }
    },
    "media": {
      "type": "library",
      "root": "libs/media",
      "entryFile": "index",
      "sourceRoot": "libs/media/src",
      "compilerOptions": {
        "tsConfigPath": "libs/media/tsconfig.lib.json"
      }
    }
  }
}