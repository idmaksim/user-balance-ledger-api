{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@app/permissions": [
        "libs/permissions/src"
      ],
      "@app/permissions/*": [
        "libs/permissions/src/*"
      ],
      "@app/users": [
        "libs/users/src"
      ],
      "@app/users/*": [
        "libs/users/src/*"
      ],
      "@app/password": [
        "libs/password/src"
      ],
      "@app/password/*": [
        "libs/password/src/*"
      ],
      "@app/token": [
        "libs/token/src"
      ],
      "@app/token/*": [
        "libs/token/src/*"
      ],
      "@app/common": [
        "libs/common/src"
      ],
      "@app/common/*": [
        "libs/common/src/*"
      ],
      "@app/prisma": [
        "libs/prisma/src"
      ],
      "@app/prisma/*": [
        "libs/prisma/src/*"
      ],
      "@app/role": [
        "libs/role/src"
      ],
      "@app/role/*": [
        "libs/role/src/*"
      ],
      "@app/sessions": [
        "libs/sessions/src"
      ],
      "@app/sessions/*": [
        "libs/sessions/src/*"
      ],
      "@app/media": [
        "libs/media/src"
      ],
      "@app/media/*": [
        "libs/media/src/*"
      ],
      "@app/products": [
        "libs/products/src"
      ],
      "@app/products/*": [
        "libs/products/src/*"
      ]
    }
  }
}